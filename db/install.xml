<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/facilitators/db" VERSION="20161221" COMMENT="XMLDB file for Moodle local/facilitators"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="attendant" COMMENT="attendant">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="siape" TYPE="char" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="siape"/>
        <FIELD NAME="cpf" TYPE="char" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="cpf"/>
        <FIELD NAME="funcao" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="funcao"/>
        <FIELD NAME="curso" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="curso"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>